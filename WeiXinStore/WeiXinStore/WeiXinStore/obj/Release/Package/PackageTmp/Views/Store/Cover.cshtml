@{
    ViewBag.Title = "欢迎光临";
    Layout = null;
}
<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width,initial-scale=1,user-scalable=no" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/styles/bootstrap.min.css")
    @Styles.Render("~/Content/styles/cover.css")
</head>
<body >
    <div class="site-wrapper" >
        <div class="site-wrapper-inner" >
            <div class="cover-container" >
                <div class="masthead clearfix">
                    <div class="inner" > 
                        <nav>
                            <ul class="nav masthead-nav">
                                <li><a href="~/Store/About">关于</a></li>
                            </ul>
                        </nav>
                    </div>
                </div>

                <div class="inner cover">
 

                    <h1 class="cover-heading" style="margin-bottom:2em;">你好音乐</h1>
                    <p class="lead"><a href="~/Store/Index" class="btn btn-lg btn-default">让我先看看</a></p>
                </div>
            </div>
        </div>
      
    </div>
    @*<footer class="footer">
        <div class="container">
            <p class="text-muted">cover template for <a href="#">bootstrap</a>,by<a href="#"> Gao's</a></p>
        </div>
    </footer>*@
    <script src="~/Scripts/jquery.min.js"></script>
    <script>
        $(document).ready(function () {
            function getUrlParam(name) {
                var reg = new RegExp("(^|&)" + name + "=([^&]*)(&|$)");
                var r = window.location.search.substr(1).match(reg);
                if (r != null) return unescape(r[2]); return null;
            }
            var code = getUrlParam("code");
            $.ajax({
                url: "CodeToToken",
                type: "POST",
                dataType: "JSON",
                data: { "code": code },
                success: function (data) {
                }
            })
        })
    </script>
</body>
</html>